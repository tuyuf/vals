// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model AlbumEntry {
  id          String   @id @default(cuid())
  albumId     String
  section     String
  placeName   String
  photoUrl    String
  description String
  moodBefore  Int?
  moodAfter   Int?
  createdAt   DateTime @default(now())

  @@index([albumId])
}
